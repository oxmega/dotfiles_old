! vim: nowrap
!

#include "/home/ox/.config/colors"

URxvt.termName: 		    rxvt-256color
URxvt.scrollBar:		    false
URxvt.scrollTtyOutput:  	false
URxvt*scrollTtyKeypress:  	true
URxvt*scrollWithBuffer:  	false
URxvt.keysym.Shift-Up:  	command:\033]720;1\007
URxvt.keysym.Shift-Down:  	command:\033]721;1\007
URxvt.iso14755:  			false
URxvt.iso14755_52: 			false

URxvt.perl-ext-common:  	clipboard,url-select
URxvt.url-launcher:  		qute
URxvt.clipboard.copycmd:  xclip -i -selection clipboard
URxvt.clipboard.pastecmd: xclip -o -selection clipboard
URxvt.keysym.M-t:           perl:url-select:select_next
URxvt.keysym.M-s            perl:keybaord-select:search
URxvt.url-select.autocopy:  true
URxvt.url-select.underline: true
URxvt.matcher.rend.0:  		Uline Bold fg5

URxvt.geometry:				80x20

URxvt.font:			        xft:DejaVu\ Sans\ Mono:autohint=0:hinting=1:size=9,xft:DejaVuSansMono\ Nerd\ Font-10:style=Book
Urxvt.boldFont:			    xft:DejaVu\ Sans\ Mono:style=Bold:autohint=0:hinting=0:size=8,xft:DejaVuSansMono\ Nerd\ Font-10:style=Book
Urxvt.italicFont:		    xft:DejaVu\ Sans\ Mono:style=Oblique:autohint=0:hinting=0:size=8,xft:DejaVuSansMono\ Nerd\ Font-10:style=Book
URxvt*colorUL:			    #e8b265
URxvt*lineSpace:		    1
URxvt.letterSpace:		    0
URxvt.intensityStyles:		0
URxvt.internalBorder:	    10

Xft.autohint:			    0
Xft.lcdfilter:			    lcddefault
Xft.hintstyle:			    hintfull
Xft.hinting:		        1
Xft.antialias:			    1
Xft.rgba:			        rgb
Xft.dpi:			        96

! Theme
URxvt.colorIT:	            #5F875F
URxvt.colorBD:	            #5F875F
URxvt.colorUL:	            #5F875F
URxvt.cursorColor:          #746054
!ncmpcpp*background:        #283f36
!ncmpcpp*foreground:        fg
! special
!.borderColor:             #efefef

! Colors
*background:   bg
*foreground:   fg
*color0:      blk
*color8:      bblk
*color1:      red
*color9:      bred
*color2:      grn
*color10:     bgrn
*color3:      ylw
*color11:     bylw
*color4:      blu
*color12:     bblu
*color5:      mag
*color13:     bmag
*color6:      cyn
*color14:     bcyn
*color7:      wht
*color15:     bwht

!vim:foldmethod=marker:foldlevel=0 syntax=muttrc textwidth=100
