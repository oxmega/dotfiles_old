[options]
allow_bold = true
audible_bell = false
bold_is_bright = true
clickable_url = true
dynamic_title = true
font = Dina, regular 9
eullscreen = false
#icon_name = terminal
mouse_autohide = false
scroll_on_output = true
scroll_on_keystroke = true
# Length of the scrollback buffer, 0 disabled the scrollback buffer
# and setting it to a negative value means "infinite scrollback"
scrollback_lines = 10000
#search_wrap = true
#urgent_on_bell = true
#hyperlinks = false

# $BROWSER is used by default if set, with xdg-open as a fallback
browser = surf

# "system", "on" or "off"
cursor_blink = on

# "block", "underline" or "ibeam"
cursor_shape = block

# Hide links that are no longer valid in url select overlay mode
#filter_unmatched_urls = true

# Emit escape sequences for extra modified keys
#modify_other_keys = false

# set size hints for the window
#size_hints = false

# "off", "left" or "right"
#scrollbar = off

[colors]
cursor = #ed9864
cursor_foreground = #2f3339

foreground = #FFFFFF
foreground_bold = #ffffff
background = #2F343F

# 20% background transparency (requires a compositor)
#background = rgba(47, 52, 63, 100)

# If unset, will reverse foreground and background
highlight = #4c566a

# Colors from color0 to color254 can be set

# Black, Gray, Silver, White
color0 = #1d2a30
color8 = #456472
color7 = #c0c5ce
color15 = #eff1f5

# Red
color1 = #a66363
color9 = #c27171

# Green
color2 = #63a690
color10 = #6dc2a3

# Yellow
color3 = #a6a663
color11 = #bfc271

# Blue
color4 = #6385a6
color12 = #719bc2

# Purple
color5 = #bf9c86
color13 = #bf9c86

# Teal
color6 = #63a69b
color14 = #71c2af

[hints]
font = Monospace 10
foreground = #dcdccc
background = #3f3f3f
#active_foreground = #e68080
#active_background = #3f3f3f
padding = 15
#border = #3f3f3f
border_width = 15
#roundness = 2.0

# vim: ft=dosini cms=#%s
